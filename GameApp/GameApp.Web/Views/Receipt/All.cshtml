@using GameApp.Web.Models.Receipt
@model AllReceiptViewModel
@{
    ViewData["Title"] = "All Receipts";
}
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>

    </tr>
  </thead>
  <tbody>
      @foreach(var item in Model.Receipts)
        {
            <tr>
                <th><a class="table-button" href="~/Receipt/@item.Id">@item.Date</a></th>
            </tr>
        }

   
  </tbody>
</table>